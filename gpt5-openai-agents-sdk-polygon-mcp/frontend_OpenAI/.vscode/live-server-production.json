{
  "liveServer.settings.root": "./dist",
  "liveServer.settings.port": 5502,
  "liveServer.settings.host": "localhost",
  "liveServer.settings.CustomBrowser": "chrome", 
  "liveServer.settings.AdvanceCustomBrowserCmdLine": "chrome --incognito --remote-debugging-port=9224 --user-data-dir=/tmp/chrome-production-session",
  "liveServer.settings.NoBrowser": false,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": false,
  "liveServer.settings.mount": [["/", "./dist"]],
  "liveServer.settings.file": "index.html",
  "liveServer.settings.wait": 100,
  "liveServer.settings.fullReload": false,
  "liveServer.settings.useBrowserPreview": false,
  "liveServer.settings.useLocalIp": true,
  "liveServer.settings.showOnStatusbar": true,
  "liveServer.settings.ignoreFiles": [
    ".vscode/**",
    "**/node_modules/**",
    "**/src/**", 
    "**/*.map",
    "**/.env*",
    "**/package*.json",
    "**/tsconfig*.json",
    "**/vite.config*",
    "**/README.md",
    "**/.gitignore",
    "**/test*",
    "**/*.test.*",
    "**/*.spec.*"
  ],
  "liveServer.settings.https": {
    "enable": false,
    "cert": "",
    "key": "",
    "passphrase": ""
  },
  "liveServer.settings.proxy": [
    ["/api", "http://localhost:8000"]
  ],
  "liveServer.settings.spa": true,
  "liveServer.settings.cors": true,
  "liveServer.settings.headers": {
    "Service-Worker-Allowed": "/",
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization"
  }
}